{
    "name": "danvaly/anaf_search",
    "type": "library",
    "version": "1.0.0",
    "description": "API ANAF pentru verificarea contribuabililor care sunt inregistrati conform art. 316 din Codul Fiscal. Compatibil cu Laravel 10+",
    "authors": [
        {
            "name": "Valentin Ghica",
            "email": "vali@danvaly.ro",
            "homepage": "https://www.danvaly.ro",
            "role": "Developer"
        }
    ],
    "keywords": [
        "api anaf",
        "verificare tva",
        "inregistrat tva",
        "webservie anaf",
        "date firma",
        "mfinante"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "laravel/framework": "^10.0",
        "php": "^8.1.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Danvaly\\AnafSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danvaly\\AnafSearch\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Anaf": "Danvaly\\AnafSearch\\Facade"
            }
        }
    }
}
